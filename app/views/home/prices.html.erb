
  

    <% if @symbol %>
    <% if @quote["Response"] %>
    <h4> Invalid search please try again.</h4>
    <% else %>
      <% @quote["DISPLAY"].each do |key, value| %>
        <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <h1 class="display-4"><%= key %></h1>
            Price: <%= value["USD"]["PRICE"] %><br/>
            24hr High: <%= value["USD"]["HIGHDAY"] %><br/>
            24hr Low: <%= value["USD"]["LOWDAY"] %><br/>
            Market Cap: <%= value["USD"]["MKTCAP"] %><br/>
      </div>  
<% end %>
<% end %>

<% else %>
        <h4> Search again to get most recent price.... </h4>
<% end %>
